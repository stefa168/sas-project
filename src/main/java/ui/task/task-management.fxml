<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.FlowPane?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/10.0.2-internal" fx:controller="ui.task.TaskManagement"
            fx:id="containerPane">
    <center>
        <TreeView fx:id="eventList" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER"/>
    </center>
    <bottom>
        <FlowPane alignment="CENTER" columnHalignment="CENTER" hgap="10.0" prefHeight="53.0" prefWidth="557.0"
                  BorderPane.alignment="CENTER">
            <children>
                <Button mnemonicParsing="false" text="Nuovo"/>
                <Button fx:id="apriButton" disable="true" text="Apri"/>
                <Button text="Fine" onAction="#endMenuManagement"/>
            </children>
        </FlowPane>
    </bottom>
</BorderPane>